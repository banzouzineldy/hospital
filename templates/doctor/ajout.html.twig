       <script>
   $(document).ready(function(){
	 $('#form').submit(function(e){
		e.preventDefault();
        var nom_doctor=$('#form #nom').val();
	   var prenom_doctor=$('#form #prenom').val();
	   var datenaissance_doctor=$('#form #datenaissance').val();
	   var email_doctor=$('#form #email').val();
	   var pays_doctor=$('#form #pays').val();
	   var capitale_doctor=$('#capitale').val();
	   var adresse_doctor=$('#form #adresse').val();
	   var telephone_doctor=$('#telephones').val();
		var genre_doctor=$('#form #genre').val();  
        var specialite_doctor=$('#form #specialite').val(); 
       var files_doctors= document.getElementById('file').files[0]; 
		let url ="{{path('app_doctores')}}";
		var formData = new FormData();
        formData.append('nom',nom_doctor);
        formData.append('prenom' ,prenom_doctor);
        formData.append('email',email_doctor);
        formData.append('pays',pays_doctor);
        formData.append('capitale',capitale_doctor);
        formData.append('adresse',adresse_doctor);
        formData.append('telephone',telephone_doctor);
        formData.append('genre' ,genre_doctor); 
        formData.append('specialite',specialite_doctor)
        formData.append('image',files_doctors);
		$.ajax({
			method:"POST",  
			data:formData,
			 url:"/update/image",
			cache: false,
			dataType:'json',
		    processData: false,
            url:'/doctore',
			success:function(data) { 
			alert(data.message);    
			},
			error:function(error) {
			console.log(error.responseText)   
			}
		});  
		   
	});
	
 }) ; 
 










       
       
