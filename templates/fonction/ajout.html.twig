    <script>

    $(document).ready(function(){  
	$('#form').submit(function(e){
	   e.preventDefault();
	   alert()
	   var libelle_fonction=$('#form #libelle').val();
	   
	   var $form={
		   libelle:libelle_fonction,
		  
      };
	   console.log(libelle_fonction)
	  
	   $.ajax({
		   method:'POST',
		   dataType:'json',
		   url:'/ajout/fonction',
			data:JSON.stringify($form),
			success:function(data) { 
			   if (data.code==1) {
			   window.location.href='/fonction' 
				   
			   }	    
		   },
		   error:function(error) {
			   console.log(error.responseText)   
		   }

	   });

   }); 	 

});  
