  <script>
    
     $(document).on('click','.deletexamen',function(){
	
	var id=$(this).attr("id");
	
	let url ="{{path('app_examen_delete', {id :'id_' })}}";
	url = url.replace("id_", id);
	$('.examen').click(function(){
	   $.ajax({
	   method:"POST",
	  data:{id:id},
	   url:url, 
		dataType:'json',
	   success:function(data) { 
		   if (data.code=="1") { 
		   window.location.href = "{{path('app_examen_liste') }}"; 
			   
		   }                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
	   },
	   error:function(error) {
		   console.log(error.responseText)   
	   }
   });

	});
   
});
    
    
    </script>

